# WireMock Configuration
WIREMOCK_PORT=8080
WIREMOCK_OPTIONS=--global-response-templating --verbose

# OpenAPI Spec Configuration
OPENAPI_SPEC_FILE=open-api-spec.yaml

# Output Configuration
MAPPINGS_DIR=./wiremock/mappings
FILES_DIR=./wiremock/__files

# Generator Configuration
GENERATE_RESPONSES=true
RESPONSE_TEMPLATING=true
DEFAULT_RESPONSE_STATUS=200
